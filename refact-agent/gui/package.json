  "main": "dist/chat/index.umd.cjs",
  "module": "dist/chat/index.js",
  "exports": {
    ".": {
      "import": "./dist/chat/index.js",
      "require": "./dist/chat/index.umd.cjs",
      "types": "./dist/chat/index.d.ts"
    },
    "./dist/events": {
      "import": "./dist/events/index.js",
